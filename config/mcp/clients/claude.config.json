{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${MCP_FS_ROOT}"
      ]
    },
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"],
      "env": {
        "GIT_WORKDIR": "${MCP_GIT_ROOT}"
      }
    },
    "context_engine": {
      "command": "D:\\\\project\\\\context_engine\\\\.venv\\\\Scripts\\\\python.exe",
      "args": [
        "D:\\\\project\\\\context_engine\\\\mcp\\\\server.py"
      ],
      "env": {
        "PG_DSN": "postgres://codex:codex@127.0.0.1:5433/codex",
        "PG_DSN_RO": "postgres://codex:codex@127.0.0.1:5433/codex",
        "NEO4J_URI": "bolt://127.0.0.1:7687",
        "NEO4J_URI_RO": "bolt://127.0.0.1:7687",
        "NEO4J_USER": "neo4j",
        "NEO4J_USER_RO": "neo4j",
        "NEO4J_PASS": "codex1234",
        "NEO4J_PASS_RO": "codex1234",
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    }
  }
}
